{
  "presets": [
    "react",
    ["es2015", {"modules": false, "loose": true}]
  ],
  "plugins": [
        ["transform-object-rest-spread", {"useBuiltIns": true}]
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "server": {
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
